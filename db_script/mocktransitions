* markx -> NEWFILE statements ENDFILE

* statements -> statement statements {ENDFILE}
* statements -> {ENDFILE}

* statement -> NEWLINE ENDLINE {NEWLINE,ENDFILE}

* statement -> NEWLINE POUND inlines POUND ENDLINE {NEWLINE,ENDFILE}
* statement -> NEWLINE POUND SINGLESPACE inlines ENDLINE {NEWLINE,ENDFILE}
* statement -> NEWLINE POUND POUND SINGLESPACE inlines ENDLINE {NEWLINE,ENDFILE}
* statement -> NEWLINE POUND POUND POUND SINGLESPACE inlines ENDLINE {NEWLINE,ENDFILE}
* statement -> NEWLINE POUND POUND POUND POUND SINGLESPACE inlines ENDLINE {NEWLINE,ENDFILE}
* statement -> NEWLINE POUND POUND POUND POUND POUND SINGLESPACE inlines ENDLINE {NEWLINE,ENDFILE}
* statement -> NEWLINE POUND POUND POUND POUND POUND POUND SINGLESPACE inlines ENDLINE {NEWLINE,ENDFILE}

* statement -> NEWLINE inlines ENDLINE {NEWLINE,ENDFILE}

* statement -> NEWLINE RIGHTANGLE RIGHTANGLE RIGHTANGLE ENDLINE lines LEFTANGLE LEFTANGLE LEFTANGLE {NEWLINE,ENDFILE}
* statement -> NEWLINE ESC ESC ESC SINGLESPACE WORD ENDLINE codelines {NEWLINE,ENDFILE}
* statement -> NEWLINE CARET CARET CARET ENDLINE lines CARET CARET CARET {NEWLINE,ENDFILE}

* statement -> list {NEWLINE,ENDFILE}

* statement -> NEWLINE MINUS MINUS MINUS ENDLINE {NEWLINE,ENDFILE}
* statement -> NEWLINE EQUAL EQUAL EQUAL ENDLINE {NEWLINE,ENDFILE}
* statement -> NEWLINE SQUIGGLY SQUIGGLY SQUIGGLY ENDLINE {NEWLINE,ENDFILE}

* statement -> NEWLINE CARET CARET CARET SINGLESPACE LEFTANGLE ENDLINE lines CARET CARET CARET {NEWLINE,ENDFILE}
* statement -> NEWLINE CARET CARET CARET SINGLESPACE RIGHTANGLE ENDLINE lines CARET CARET CARET {NEWLINE,ENDFILE}
* statement -> NEWLINE CARET CARET CARET SINGLESPACE EQUAL ENDLINE lines CARET CARET CARET {NEWLINE,ENDFILE}
* statement -> NEWLINE CARET CARET CARET SINGLESPACE EQUAL EQUAL ENDLINE lines CARET CARET CARET {NEWLINE,ENDFILE}
* statement -> NEWLINE CARET CARET CARET SINGLESPACE EQUAL SINGLESPACE EQUAL ENDLINE lines CARET CARET CARET {NEWLINE,ENDFILE}
* statement -> NEWLINE CARET CARET CARET SINGLESPACE EQUAL SINGLESPACE EQUAL SINGLESPACE EQUAL ENDLINE lines CARET CARET CARET {NEWLINE,ENDFILE}


* list -> NEWLINE LOWERROMAN SINGLESPACE inlines ENDLINE sublist {NEWLINE,ENDFILE}
* list -> NEWLINE UPPERROMAN SINGLESPACE inlines ENDLINE sublist {NEWLINE,ENDFILE}
* list -> NEWLINE RIGHTANGLE SINGLESPACE inlines ENDLINE sublist {NEWLINE,ENDFILE}
* list -> NEWLINE MINUS SINGLESPACE inlines ENDLINE sublist {NEWLINE,ENDFILE}
* list -> NEWLINE PLUS SINGLESPACE inlines ENDLINE sublist {NEWLINE,ENDFILE}
* list -> NEWLINE ASTERISK SINGLESPACE inlines ENDLINE sublist {NEWLINE,ENDFILE}
* list -> NEWLINE LOWERA SINGLESPACE inlines ENDLINE sublist {NEWLINE,ENDFILE}
* list -> NEWLINE UPPERA SINGLESPACE inlines ENDLINE sublist {NEWLINE,ENDFILE}
* list -> NEWLINE NUMONE SINGLESPACE inlines ENDLINE sublist {NEWLINE,ENDFILE}


* sublist -> NEWLINE LOWERROMAN SINGLESPACE inlines ENDLINE sublist {NEWLINE,ENDFILE}
* sublist -> NEWLINE UPPERROMAN SINGLESPACE inlines ENDLINE sublist {NEWLINE,ENDFILE}
* sublist -> NEWLINE RIGHTANGLE SINGLESPACE inlines ENDLINE sublist {NEWLINE,ENDFILE}
* sublist -> NEWLINE MINUS SINGLESPACE inlines ENDLINE sublist {NEWLINE,ENDFILE}
* sublist -> NEWLINE PLUS SINGLESPACE inlines ENDLINE sublist {NEWLINE,ENDFILE}
* sublist -> NEWLINE ASTERISK SINGLESPACE inlines ENDLINE sublist {NEWLINE,ENDFILE}
* sublist -> NEWLINE LOWERA SINGLESPACE inlines ENDLINE sublist {NEWLINE,ENDFILE}
* sublist -> NEWLINE UPPERA SINGLESPACE inlines ENDLINE sublist {NEWLINE,ENDFILE}
* sublist -> NEWLINE NUMONE SINGLESPACE inlines ENDLINE sublist {NEWLINE,ENDFILE}
* sublist -> NEWLINE ENDLINE {NEWLINE,ENDFILE}
* sublist -> {ENDFILE}


* lines -> NEWLINE inlines ENDLINE sublines {LEFTANGLE}

* sublines -> NEWLINE inlines ENDLINE sublines {LEFTANGLE}
* sublines -> {LEFTANGLE}

* inlines -> inline subinlines {LEFTANGLE,RIGHTSQUAREBRACKET,SPACE,ENDLINE,ESC,ASTERISK,UNDERSCORE,MINUS}

* subinlines -> inline subinlines {LEFTANGLE,RIGHTSQUAREBRACKET,SPACE,ENDLINE,ESC,ASTERISK,UNDERSCORE,MINUS}
* subinlines -> {LEFTANGLE,RIGHTSQUAREBRACKET,SPACE,ENDLINE,ESC,ASTERISK,UNDERSCORE,MINUS}


* inline -> words {LEFTANGLE,RIGHTSQUAREBRACKET,SPACE,ENDLINE,ESC,ASTERISK,UNDERSCORE,MINUS}
* inline -> ASTERISK ASTERISK words ASTERISK ASTERISK {LEFTANGLE,RIGHTSQUAREBRACKET,SPACE,ENDLINE,ESC,ASTERISK,UNDERSCORE,MINUS}

* words -> WORD subwords {LEFTANGLE,RIGHTSQUAREBRACKET,SPACE,ENDLINE,ESC,ASTERISK,UNDERSCORE,MINUS}

* subwords -> {LEFTANGLE,RIGHTSQUAREBRACKET,SPACE,ENDLINE,ESC,ASTERISK,UNDERSCORE,MINUS}
