* markx -> NEWFILE statements ENDFILE

* statements -> statement statements {ENDFILE}
* statements -> {ENDFILE}

* statement -> NEWLINE ENDLINE {NEWLINE,ENDFILE}
* statement -> NEWLINE RIGHTANGLE RIGHTANGLE RIGHTANGLE ENDLINE lines NEWLINE LEFTANGLE LEFTANGLE LEFTANGLE ENDLINE {NEWLINE,ENDFILE}


* lines -> NEWLINE inlines ENDLINE sublines {NEWLINE}

* sublines -> NEWLINE inlines ENDLINE sublines {NEWLINE}
* sublines -> {NEWLINE}

* inlines -> inline subinlines {RIGHTSQUAREBRACKET,SPACE,ENDLINE,ESC,ASTERISK,UNDERSCORE,MINUS}

* subinlines -> inline subinlines {RIGHTSQUAREBRACKET,SPACE,ENDLINE,ESC,ASTERISK,UNDERSCORE,MINUS}
* subinlines -> {RIGHTSQUAREBRACKET,SPACE,ENDLINE,ESC,ASTERISK,UNDERSCORE,MINUS}


* inline -> words {RIGHTSQUAREBRACKET,SPACE,ENDLINE,ESC,ASTERISK,UNDERSCORE,MINUS}


* words -> WORD subwords {RIGHTSQUAREBRACKET,SPACE,ENDLINE,ESC,ASTERISK,UNDERSCORE,MINUS}

* subwords -> {RIGHTSQUAREBRACKET,SPACE,ENDLINE,ESC,ASTERISK,UNDERSCORE,MINUS}
